@prefix : <http://www.semanticweb.org/samragu/ontologies/comercio-electronico#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/samragu/ontologies/comercio-electronico> .

<http://www.semanticweb.org/samragu/ontologies/comercio-electronico> rdf:type owl:Ontology ;
                                                                      owl:versionIRI <http://www.semanticweb.org/samragu/ontologies/comercio-electronico/1.0.0> .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#busca
:busca rdf:type owl:ObjectProperty ;
       rdfs:domain :BuscarProductos ;
       rdfs:range :Busqueda .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#con_cobro
:con_cobro rdf:type owl:ObjectProperty ;
           rdfs:domain :Pedido ;
           rdfs:range :Cobro .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#con_destino
:con_destino rdf:type owl:ObjectProperty ;
             rdfs:domain :Envio ,
                         :Lote ;
             rdfs:range :Lugar .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#con_origen
:con_origen rdf:type owl:ObjectProperty ;
            rdfs:domain :Envio ,
                        :Lote ;
            rdfs:range :CentroLogistico ,
                       :ComercioExterno .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#con_reembolso
:con_reembolso rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Devolucion ;
               rdfs:range :Reembolso .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#de_devolucion
:de_devolucion rdf:type owl:ObjectProperty ;
               rdfs:domain :ContratarRecogida ,
                           :InformarRecogidaProductos ,
                           :RespuestaInformacionTransporteRecogida ,
                           :RespuestaResolucionDevolucion ,
                           :SolicitarDevolucionProductos ,
                           :SolicitarInformacionTransporteRecogida ;
               rdfs:range :Devolucion .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#de_pedido
:de_pedido rdf:type owl:ObjectProperty ;
           rdfs:domain :InformarEnvioPedido ,
                       :LineaEnvio ,
                       :Pedido ;
           rdfs:range :Pedido .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#devolver_a
:devolver_a rdf:type owl:ObjectProperty ;
            rdfs:domain :SolicitarInformacionTransporteRecogida ;
            rdfs:range :CentroLogistico ,
                       :ComercioExterno .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#envia_pedido
:envia_pedido rdf:type owl:ObjectProperty ;
              rdfs:domain :RealizarEnvio ;
              rdfs:range :Pedido .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#esta_en
:esta_en rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :CentroLogistico ;
         rdfs:range :Lugar .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#hace_valoracion
:hace_valoracion rdf:type owl:ObjectProperty ;
                 rdfs:domain :RespuestaValoracionProducto ;
                 rdfs:range :Valoracion .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#hecha_por
:hecha_por rdf:type owl:ObjectProperty ;
           rdfs:domain :Valoracion ;
           rdfs:range :Asistente .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#ofertado_en
:ofertado_en rdf:type owl:ObjectProperty ;
             rdfs:domain :Producto ;
             rdfs:range :Oferta .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#ofrece
:ofrece rdf:type owl:ObjectProperty ;
        rdfs:domain :ComercioExterno ;
        rdfs:range :Oferta .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#producto
:producto rdf:type owl:ObjectProperty ;
          rdfs:domain :LineaProducto ;
          rdfs:range :Producto .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#recoger_en
:recoger_en rdf:type owl:ObjectProperty ;
            rdfs:domain :Devolucion ;
            rdfs:range :Lugar .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#recomienda
:recomienda rdf:type owl:ObjectProperty ;
            rdfs:domain :RecomendarProductos ;
            rdfs:range :Recomendacion .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#recomienda_a
:recomienda_a rdf:type owl:ObjectProperty ;
              rdfs:domain :Recomendacion ;
              rdfs:range :Asistente .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#recomienda_producto
:recomienda_producto rdf:type owl:ObjectProperty ;
                     rdfs:domain :Recomendacion ;
                     rdfs:range :Producto .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#se_entrega_en
:se_entrega_en rdf:type owl:ObjectProperty ;
               rdfs:domain :Pedido ;
               rdfs:range :Lugar .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_busqueda
:tiene_busqueda rdf:type owl:ObjectProperty ;
                rdfs:domain :ActualizarHistorialBusqueda ;
                rdfs:range :Busqueda .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_cobro
:tiene_cobro rdf:type owl:ObjectProperty ;
             rdfs:domain :Cobrar ,
                         :RespuestaCobro ;
             rdfs:range :Cobro .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_en_inventario
:tiene_en_inventario rdf:type owl:ObjectProperty ;
                     rdfs:domain :CentroLogistico ;
                     rdfs:range :LineaProducto .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_envios
:tiene_envios rdf:type owl:ObjectProperty ;
              rdfs:domain :Pedido ;
              rdfs:range :Envio .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_factura
:tiene_factura rdf:type owl:ObjectProperty ;
               rdfs:domain :InformarFacturaPedido ,
                           :Pedido ;
               rdfs:range :Factura .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_linea_busqueda
:tiene_linea_busqueda rdf:type owl:ObjectProperty ;
                      rdfs:domain :Busqueda ;
                      rdfs:range :LineaBusqueda .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_linea_envio
:tiene_linea_envio rdf:type owl:ObjectProperty ;
                   rdfs:domain :SolicitarEnvio ;
                   rdfs:range :LineaEnvio .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_linea_oferta
:tiene_linea_oferta rdf:type owl:ObjectProperty ;
                    rdfs:domain :Factura ,
                                :InformarProductosPedidos ,
                                :Pedido ,
                                :RespuestaProductos ;
                    rdfs:range :LineaOferta .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_linea_producto
:tiene_linea_producto rdf:type owl:ObjectProperty ;
                      rdfs:domain :Envio ,
                                  :Lote ;
                      rdfs:range :LineaProducto .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_lote
:tiene_lote rdf:type owl:ObjectProperty ;
            rdfs:domain :ContratarEnvio ,
                        :RespuestaInformacionTransporteEnvio ,
                        :SolicitarInformacionTransporteEnvio ;
            rdfs:range :Lote .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_marca
:tiene_marca rdf:type owl:ObjectProperty ;
             rdfs:domain :Modelo ;
             rdfs:range :Marca .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_metodo_pago
:tiene_metodo_pago rdf:type owl:ObjectProperty ;
                   rdfs:domain :Pedido ;
                   rdfs:range :MetodoPago .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_modelo
:tiene_modelo rdf:type owl:ObjectProperty ;
              rdfs:domain :Producto ;
              rdfs:range :Modelo .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_oferta
:tiene_oferta rdf:type owl:ObjectProperty ;
              rdfs:domain :ActualizarInformacionProductos ;
              rdfs:range :Oferta .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_pedido
:tiene_pedido rdf:type owl:ObjectProperty ;
              rdfs:domain :ActualizarHistorialPedidos ,
                          :RealizarPedido ;
              rdfs:range :Pedido .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_por_enviar
:tiene_por_enviar rdf:type owl:ObjectProperty ;
                  rdfs:domain :CentroLogistico ;
                  rdfs:range :Lote .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_reembolso
:tiene_reembolso rdf:type owl:ObjectProperty ;
                 rdfs:domain :Reembolsar ,
                             :RespuestaReembolso ;
                 rdfs:range :Reembolso .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#tiene_valoracion
:tiene_valoracion rdf:type owl:ObjectProperty ;
                  rdfs:domain :Producto ;
                  rdfs:range :Valoracion .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#valora_producto
:valora_producto rdf:type owl:ObjectProperty ;
                 rdfs:domain :SolicitarValoracionProducto ;
                 rdfs:range :Producto .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#cantidad
:cantidad rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :Linea ,
                      :Oferta ,
                      :Producto ;
          rdfs:range xsd:nonNegativeInteger ;
          rdfs:comment "La cantidad se expresa como el número de unidades de la clase a la cual pertenece."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#categoria
:categoria rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain :LineaBusqueda ,
                       :Producto ;
           rdfs:range xsd:string ;
           rdfs:comment "La categoria representa un aspecto característico de Producto por el cual se le puede buscar."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#ciudad
:ciudad rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:domain :Lugar ;
        rdfs:range xsd:string ;
        rdfs:comment "La ciudad identifica un lugar en concreto, siendo esta única."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#cvv
:cvv rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:domain :TargetaCredito ;
     rdfs:range xsd:string ;
     rdfs:comment "El cvv es el Código de Valor de Verificación de una tarjeta de crédito."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#descripcion
:descripcion rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :MetodoPago ,
                         :Producto ;
             rdfs:range xsd:string ;
             rdfs:comment "La descripción consiste en una explicación más detallada del concepto al cual pertenece."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#estado
:estado rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:domain :Devolucion ,
                    :Pedido ,
                    :RespuestaCobro ,
                    :RespuestaReembolso ;
        rdfs:range xsd:string ;
        rdfs:comment "El estado representa la circunstancia en la que se encuentra el concepto al cual pertenece."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#fecha_caducidad
:fecha_caducidad rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain :TargetaCredito ;
                 rdfs:range xsd:dateTime ;
                 rdfs:comment "La fecha de caducidad es la fecha en la que una tarjeta de crédito deja de ser válida."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#gestion_envio
:gestion_envio rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Oferta ;
               rdfs:range xsd:string ;
               rdfs:comment "Gestión envio expresa si una oferta debe gestionarse de forma interna o externa."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#identificador
:identificador rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Actor ,
                           :Agente ,
                           :Cobro ,
                           :Devolucion ,
                           :Envio ,
                           :Oferta ,
                           :Pedido ,
                           :Reembolso ;
               rdfs:range xsd:positiveInteger ;
               rdfs:comment "El identificador se usa para poder distinguir entre diferentes instancias del mismo concepto."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#importe_total
:importe_total rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Devolucion ,
                           :Factura ,
                           :Pago ,
                           :Pedido ;
               rdfs:range xsd:float ;
               rdfs:comment "El importe total se expresa como la suma de diferentes cantidades de dinero que tienen un fin común."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#instrucciones
:instrucciones rdf:type owl:DatatypeProperty ;
               rdfs:domain :InformarRecogidaProductos ;
               rdfs:range xsd:string ;
               rdfs:comment "Serie de pasos a seguir para llevar a cabo un objetivo"@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#nombre
:nombre rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:domain :Marca ,
                    :Modelo ,
                    :Producto ;
        rdfs:range xsd:string ;
        rdfs:comment "El nombre se usa para designar un concepto."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#pan
:pan rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:domain :TargetaCredito ;
     rdfs:range xsd:string ;
     rdfs:comment "El pan (Personal Account Number) es el Número de la tarjeta bancaria."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#precio
:precio rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:domain :Oferta ,
                    :RespuestaInformacionTransporteEnvio ,
                    :RespuestaInformacionTransporteRecogida ;
        rdfs:range xsd:float ;
        rdfs:comment "El precio representa la cantidad de dinero que cuesta una determinada transacción."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#precio_max
:precio_max rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :precio ;
            rdfs:domain :LineaBusqueda ;
            rdfs:range xsd:float ;
            rdfs:comment "Indica el precio máximo de un intervalo."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#precio_min
:precio_min rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :precio ;
            rdfs:domain :LineaBusqueda ;
            rdfs:range xsd:float ;
            rdfs:comment "Indica el precio mínimo de un intervalo."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#prioridad
:prioridad rdf:type owl:DatatypeProperty ;
           rdfs:domain :Pedido ;
           rdfs:range xsd:float ;
           rdfs:comment "La prioridad se expresa como el número máximo de dias que deben pasar hasta que el Pedido se entregue."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#restricciones_devolucion
:restricciones_devolucion rdf:type owl:DatatypeProperty ;
                          rdfs:domain :Producto ;
                          rdfs:range xsd:string ;
                          rdfs:comment "Las restricciones de devolución de un producto representan una serie de condiciones que se deben cumplir para que se pueda tramitar la devolución como tal."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#valor
:valor rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty ;
       rdfs:domain :Valoracion ;
       rdfs:range xsd:nonNegativeInteger ;
       rdfs:comment "Cualquier valor numerico cuya semantica depende de la clase en la que se usa."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#valoracion_media
:valoracion_media rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain :Producto ;
                  rdfs:range xsd:nonNegativeInteger ;
                  rdfs:comment "Media aritmetica de todas las valoraciones hechas a un Producto"@es .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#Accion
:Accion rdf:type owl:Class ;
        rdfs:subClassOf :Comunicacion ;
        owl:disjointWith :Respuesta ;
        rdfs:comment "Acciones entre Actores y Agentes del Sistema."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#Actor
:Actor rdf:type owl:Class ;
       rdfs:comment "Actor externo al Sistema que interactua con éste." .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#ActualizarHistorialBusqueda
:ActualizarHistorialBusqueda rdf:type owl:Class ;
                             rdfs:subClassOf :Accion ;
                             rdfs:comment "El Agente BuscadorProductos envia la busqueda hecha por un Asistente al Agente Recomendador/ControlCalidad para que la almacene en el Historial búsquedas"@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#ActualizarHistorialPedidos
:ActualizarHistorialPedidos rdf:type owl:Class ;
                            rdfs:subClassOf :Accion ;
                            rdfs:comment "Al completarse y cerrarse un Pedido, el Agente Gestor pedidos lo manda al Recomendador/control calidad para que lo almacene en el Historial pedidos."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#ActualizarInformacionProductos
:ActualizarInformacionProductos rdf:type owl:Class ;
                                rdfs:subClassOf :Accion ;
                                rdfs:comment "El ComercioExterno envia un conjunto de Ofertas. Si la Oferta ya existia en el Sistema, se actualiza. Si no existia, se crea."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#AgenciaTransporte
:AgenciaTransporte rdf:type owl:Class ;
                   rdfs:subClassOf :Actor ;
                   rdfs:comment "Se encarga del envio de Lotes y la recogida de Devoluciones. El Sistema negocia con esta el envio de cada uno de los Lotes. Como habrá varias Agencias de Transporte disponibles, elegirá la más barata y conveniente."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#Agente
:Agente rdf:type owl:Class ;
        rdfs:comment "Agente que forma parte del Sistema"@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#Asistente
:Asistente rdf:type owl:Class ;
           rdfs:subClassOf :Actor ;
           rdfs:comment "Es el intermediario entre el Usuario/Cliente. Actua en su nombre y es el principal Actor del Sistema."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#BuscadorProductos
:BuscadorProductos rdf:type owl:Class ;
                   rdfs:subClassOf :Agente .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#BuscarProductos
:BuscarProductos rdf:type owl:Class ;
                 rdfs:subClassOf :Accion ;
                 rdfs:comment "Un Asistente realiza una Busqueda sobre unas categorias de Productos, indicando cantidad y un intervalo de precio para cada una de ellas."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#Busqueda
:Busqueda rdf:type owl:Class ;
          rdfs:comment "Conjunto de LineaBusqueda (categorias de Productos asociadas con cantidades e intervalos de precio)"@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#CentroLogistico
:CentroLogistico rdf:type owl:Class ;
                 rdfs:subClassOf :Agente ;
                 rdfs:comment "Almacen de Productos y Agente encargado del Envio de los Productos comprados."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#Cobrar
:Cobrar rdf:type owl:Class ;
        rdfs:subClassOf :Accion ;
        rdfs:comment "El Gestor pedidos solicita el Cobro al Proveedor de pagos."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#Cobro
:Cobro rdf:type owl:Class ;
       rdfs:subClassOf :Pago ;
       rdfs:comment "Transaccion entrante a las cuentas del Sistema"@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#ComercioExterno
:ComercioExterno rdf:type owl:Class ;
                 rdfs:subClassOf :Actor ;
                 rdfs:comment "Pone a la venta Productos en nuestro sistema en forma de Ofertas. Estas ofertas pueden estar más o menos integradas en el Sistema, pudiendo darse 2 escenarios: 1) Los Productos se encuentran en algun CentroLogistico y el Sistema se encarga de su envio. 2) Los Productos se encuentran en el ComercioExterno y por lo tanto es éste el que se encarga de su envio."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#Comunicacion
:Comunicacion rdf:type owl:Class .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#ContratarEnvio
:ContratarEnvio rdf:type owl:Class ;
                rdfs:subClassOf :Accion ;
                rdfs:comment "El CentroLogistico contrata el envio de un Lote con una AgenciaTransporte"@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#ContratarRecogida
:ContratarRecogida rdf:type owl:Class ;
                   rdfs:subClassOf :Accion ;
                   rdfs:comment "El Sistema contrata la Recogida de una Devolución con una AgenciaTransporte."@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#Devolucion
:Devolucion rdf:type owl:Class ;
            rdfs:comment "Permite a un Asistente devolver un/os producto/s y recuperar el dinero pagado por el"@es .


###  http://www.semanticweb.org/samragu/ontologies/comercio-electronico#Envio
:Envio rdf:type owl:Class ;
